    <select id="selectCfCertificateIPage" parameterType="com.hxoms.modules.passportCard.entity.param.CfCertificatePageParam"
            resultType="com.hxoms.modules.passportCard.entity.CfCertificate">
        SELECT
            cc.ID AS ID,
            orp.INCUMBENCY_STATUS AS incumbencyStatus,
            orp.RF_B0000,
            cc.A0184 AS a0184,
            cc.NAME,
            cc.PY,
            cc.SEX,
            cc.CSRQ,
            cc.GJ,
            cc.ZWCSDD,
            cc.LOCATION,
            cc.zjlx,
            cc.ZJSJRQ,
            cc.ZJHM,
            cc.QFRQ,
            cc.IS_LOCK,
            cc.MANAGE_UNIT,
            cc.YXQZ,
            cc.DQZT,
            cc.CABINET_NUM,
            cc.EXCEPTION_MESSAGE,
            cc.IS_CABINET,
            cc.SAVE_STATUS,
            cc.STORAGE_PLACE,
            cc.UPDATE_TIME
        FROM
        cf_certificate cc
          LEFT JOIN oms_reg_procpersoninfo orp ON cc.A0184 = orp.IDNUMBER
          LEFT JOIN B01 b ON orp.RF_B0000 = b.B0100
        WHERE 1=1 AND cc.IS_VALID = 0
        <if test="name != null">
            AND cc.NAME like CONCAT(CONCAT('%', #{name}), '%')
            OR cc.PY like CONCAT(CONCAT('%',#{name}),'%')
        </if>

        <if test="zjhm != null">
            AND cc.ZJHM like CONCAT(CONCAT('%', #{zjhm}), '%')
        </if>

        <if test="dqzt != null">
            AND cc.DQZT = #{dqzt}
        </if>

        <if test="isLock != null">
            AND cc.IS_LOCK = #{isLock}
        </if>

        <if test="yxqz != null">
            AND cc.YXQZ &lt;= #{yxqz}
        </if>

        <if test="b0100 != null">
            AND b.B0100 in
            <foreach item="b0100" index="index" collection="b0100" open="(" separator="," close=")">
                #{b0100}
            </foreach>
        </if>
        ORDER BY cc.update_time DESC
    </select>

    <select id="findOverduePass" parameterType="com.hxoms.modules.passportCard.entity.param.CfCertificateReminderParam"
                                    resultType="com.hxoms.modules.passportCard.entity.CfCertificateReminder">
        SELECT
        cf.*
        FROM
        cf_certificate cf
        LEFT JOIN oms_pri_apply opa ON cf.ZJHM = opa.PASSPORT_NUM
        WHERE
        cf.DQZT = 4
        OR opa.REVERT_LICENCE_TIME &lt;=NOW()


    </select>

