    <delete id="delete" parameterType="java.lang.String">

        UPDATE cf_certificate SET IS_VALID = 1 WHERE ID = #{id}

    </delete>

    <select id="findCfById" parameterType="java.lang.String"
            resultType="com.hxoms.modules.passportCard.entity.CfCertificate">
        SELECT * FROM cf_certificate WHERE ID = #{id}
    </select>

    <select id="findSuccessCf" resultType="java.lang.Integer">

        SELECT COUNT(1) FROM cf_certificate WHERE MANAGEUNIT != NULL OR MANAGEUNIT !=""

