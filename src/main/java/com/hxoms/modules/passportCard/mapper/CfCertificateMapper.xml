    <result column="ISCABINET" jdbcType="INTEGER" property="iscabinet" />

    <result column="B0100" jdbcType="VARCHAR" property="b0100" />
    <result column="B0101" jdbcType="VARCHAR" property="b0101" />
    <result column="A0215A" jdbcType="VARCHAR" property="a0215a" />
    <result column="INCUMBENCYSTATUS" jdbcType="VARCHAR" property="incumbencyStatus" />
    <result column="B0100PID" jdbcType="VARCHAR" property="b0100Pid" />
    <result column="DUTY" jdbcType="VARCHAR" property="duty" />
  </resultMap>


    <select id="selectCfCertificateIPage" parameterType="com.hxoms.modules.passportCard.entity.param.CfCertificatePageParam"
            resultType="com.hxoms.modules.passportCard.entity.param.CfCertificatePageParam">
        SELECT
        cc.ID AS ID,
        a2.A0215A as A0215A,
        orp.INCUMBENCY_STATUS AS incumbencyStatus,
        b.B0100 AS b0100,
        b.b0101 AS b0101,
        a.A0100 AS a0100,
        cc.A0184 AS a0184,
        cc.NAME,
        cc.SEX,
        cc.CSRQ,
        cc.GJ,
        cc.ZWCSDD,
        cc.LOCATION,
        cc.zjlx,
        cc.ZJSJRQ,
        cc.ZJHM,
        cc.QFRQ,
        cc.ISLOCK,
        cc.MANAGEUNIT,
        cc.YXQZ,
        cc.DQZT,
        cc.UPDATE_TIME
        FROM
        cf_certificate cc
        LEFT JOIN a01 a ON a.a0184 = cc.A0184
        LEFT JOIN a02 a2 ON a2.a0100 = a.a0100
        LEFT JOIN b01 b ON a2.a0201b = b.b0111
        LEFT JOIN oms_reg_procpersoninfo orp ON orp.A0100 = a.a0100
        WHERE 1=1 AND cc.IS_VALID = 0
        <if test="name != null">
            AND cc.NAME like CONCAT(CONCAT('%', #{name}), '%')
        </if>

        <if test="zjhm != null">
            AND cc.ZJHM like CONCAT(CONCAT('%', #{zjhm}), '%')
        </if>

        <if test="dqzt != null">
            AND cc.DQZT = #{dqzt}
        </if>

        <if test="isLock != null">
            AND cc.ISLOCK = #{isLock}
        </if>

        <if test="yxqz != null">
            AND cc.YXQZ &lt;= #{yxqz}
        </if>


        <if test="b0100s != null">
            AND b.B0100 in
            <foreach item="b0100s" index="index" collection="b0100s" open="(" separator="," close=")">
                #{b0100s}
            </foreach>
        </if>
        ORDER BY cc.update_time DESC
    </select>

    <select id="findOverduePass" parameterType="com.hxoms.modules.passportCard.entity.param.CfCertificateReminderParam"
                                    resultType="com.hxoms.modules.passportCard.entity.CfCertificateReminder">
        SELECT
        cf.*
        FROM
        cf_certificate cf
        LEFT JOIN oms_pri_apply opa ON cf.ZJHM = opa.PASSPORT_NUM
        WHERE
        cf.DQZT = 4
        OR opa.REVERT_LICENCE_TIME &lt;=ing NOW()


    </select>

