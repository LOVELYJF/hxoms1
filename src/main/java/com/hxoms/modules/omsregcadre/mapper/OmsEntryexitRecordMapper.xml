  </select>
  <select id="queryCompresultByYear" resultType="com.hxoms.modules.omsregcadre.entity.OmsEntryexitRecordVO" parameterType="java.lang.String">
      select
      count(CASE WHEN t.comparisonResult LIKE '%未经申请延期%' then 1 else null end) AS wjsqyqcount,
      count(CASE WHEN t.comparisonResult LIKE '%未经审批出%' then 1 else null end) AS wjspcount,
      count(CASE WHEN t.comparisonResult LIKE '%擅自变更%' then 1 else null end) AS szbgcount,
      (SELECT count(1) from oms_pri_apply apply where apply.IS_COMPARISON =1
      <if test="year!=null and year!=''">
          and to_char(record.COMPARISON_DATE,'yyyy') <![CDATA[<=]]>#{year}
      </if>
      ) AS yssqcount,
      (SELECT count(1) FROM `oms_entryexit_record` record where length(record.PRIAPPLY_ID)>0
      and record.PRIAPPLY_ID is not NULL
      <if test="year!=null and year!=''">
          and to_char(record.COMPARISON_DATE,'yyyy') <![CDATA[<=]]>#{year}
      </if>
      ) AS crjcount
      FROM (	SELECT record.comparison_Result as comparisonResult from oms_pri_apply apply
      left join oms_entryexit_record record on apply.PROCPERSON_ID = record.OMS_ID
      where apply.IS_ABROAD = 1
      and length(record.PRIAPPLY_ID)>0
      and record.PRIAPPLY_ID is not NULL
      ) t
  </select>

