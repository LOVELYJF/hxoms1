  <select id="selectEntryexitRecordIPage" parameterType="com.hxoms.modules.omsregcadre.entity.paramentity.OmsEntryexitRecordIPagParam" resultMap="BaseResultMap">
      select * from oms_entryexit_record where 1=1
      <if test="name!=null and name!=''">
          AND NAME LIKE '%${name}%'
      </if>
      <if test="dataSource!=null and dataSource!=''">
          AND DATA_SOURCE = #{dataSource}
      </if>
      <if test="ogeStatus!=null and ogeStatus!=''">
          AND OGE_STATUS = #{ogeStatus}
      </if>
      <if test="ogeStartDate != null and ogeEndDate!=null">
          and OGE_DATE &gt;= #{ogeStartDate} and OGE_DATE &lt;= #{ogeEndDate}
      </if>
      <if test="ogeDate != null">
          and OGE_DATE &gt;= #{ogeDate}
      </if>
      <if test="comparisonResult!=null and comparisonResult!=''">
          and COMPARISON_RESULT LIKE '%${comparisonResult}%'
      </if>
      <if test="omsId!=null and omsId!=''">
          AND OMS_ID = #{omsId}
      </if>
      <if test="ogeStatus!=null and ogeStatus!=''">
          AND OGE_STATUS = #{ogeStatus}
      </if>
      <if test="ids!=null">
          and ID in
          <foreach collection="ids" item="id" open="(" close=")" separator=",">
              #{id}
          </foreach>
      </if>
      order by OGE_DATE asc
  </select>


  <select id="getExceptionPriApply" parameterType="com.hxoms.modules.omsregcadre.entity.paramentity.OmsEntryexitRecordIPagParam" resultType="com.hxoms.modules.omsregcadre.entity.OmsEntryexitRecordVO">
      select
          rp.SURNAME as surname,
          rp.NAME as name,
          rp.SEX as sex,
          rp.WORK_UNIT as workUnit,
          rp.POST as post,
          apply.ABROAD_REASONS as abroadReasons,
          apply.ABROAD_TIME as abroadTime,
          apply.RETURN_TIME as returnTime,
          apply.GO_COUNTRY as goCountry,
          record.PRIAPPLY_ID as priapplyId,
          record.OGE_DATE as ogeDate,
          record.OGE_STATUS as ogeStatus,
          record.DESTINATION as destination,
          record.ID_TYPE as idType,
          record.ENTRACE_EXIT as entraceExit,
          record.COMPARISON_RESULT as comparisonResult,
          record.ID_NUMBER as idNumber
      from oms_pri_apply apply
      LEFT JOIN oms_reg_procpersoninfo rp on apply.PROCPERSON_ID = rp.ID
      left join oms_entryexit_record record
      on apply.PROCPERSON_ID = record.OMS_ID
      where apply.IS_ABROAD = 1
      and length(record.PRIAPPLY_ID)>0
      and record.PRIAPPLY_ID is not NULL
      and apply.COMPARISON_RESULT is not null and apply.COMPARISON_RESULT !='正常'
      and record.COMPARISON_RESULT is not null and record.COMPARISON_RESULT!='正常'
    <if test="comparisonResult!=null and comparisonResult!=''">
      and record.COMPARISON_RESULT LIKE '%${comparisonResult}%'
    </if>
    <if test="year!=null and year!=''">
      and to_char(record.COMPARISON_DATE,'YYYY')=#{year}
    </if>
    <if test="omsId!=null and omsId!=''">
      and record.OMS_ID=#{omsId}
    </if>

      order by record.OGE_DATE asc
  </select>



